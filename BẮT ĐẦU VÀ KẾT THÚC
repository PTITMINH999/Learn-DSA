#include <bits/stdc++.h>
using ll = long long;
using namespace std;
#define mod 1000000007

int main()
{
    int t; cin >> t;
    while(t--){
        string s; cin  >> s;
        if(s[0] == s[s.size() - 1]){
            cout << "YES" << endl;
        }
        else cout << "NO" << endl;
    }
    return 0;
}
