#include<bits/stdc++.h>
using namespace std;
#define faster() ios_base::sync_with_stdio(0); cin.tie(NULL); cout.tie(NULL);
#define run() int t; cin>>t; while(t--)
#define ll long long

int main(){
    faster();
    char a, cong, b, bang, c;
    cin >> a >> cong >> b >> bang >> c; 
    if (cong == '+' && bang == '=' && (a - '0') + (b - '0') == (c - '0'))
        cout << "YES";
    else
        cout << "NO";   
    return 0;
}
