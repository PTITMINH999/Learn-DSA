#include <bits/stdc++.h>
using namespace std;

#define faster ios_base::sync_with_stdio(false); cin.tie(NULL); cout.tie(NULL);
#define ll long long
#define vi vector<int>
#define vl vector<ll>
#define pii pair<int, int>
#define pll pair<ll, ll>
#define pb push_back
#define mp make_pair
#define fi first
#define se second

const int MOD = 1e9 + 7;
const int INF = 1e9;
const ll LLINF = 1e18;
const int maxN = 2e5+5;
int k = 1;
void solve(){
    int n;
    cin >> n;
    cout<<"Test "<<k<<": ";
    for(int i=2;i<=sqrt(n);i++){
        if(n%i==0){
            int cnt = 0;
            while(n%i==0){
                cnt++;
                n/=i;
            }
            cout<<i<<'('<<cnt<<') ';
        }
    }
    if(n>1){
        cout<<n<<'('<<1<<') ';
    }
    k++;
    cout<<endl;
}
int main(){
    faster;
    int t =1;
    cin>>t;
    while(t--){
        solve();
    }
}
